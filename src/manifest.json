{
  "manifest_version": 3,
  "background": {
    "service_worker": "service_worker.ts",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "content/index.ts"
      ],
      "matches": [
        "https://*/*",
        "http://*/*"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "pages/newtab/index.html"
  },
  "action": {
    "default_popup": "pages/popup/index.html"
  },
  "name": "Fresh Coupons"
}
